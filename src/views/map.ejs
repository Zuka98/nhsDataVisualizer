  <!--  Map Timeline Area --> 

<script> 
function loadWeekData(weekNum) {

  $.ajax({
        url: '/loadWeekData',
        async: 'true',
        //force to handle it as text
        dataType: "json",
        data:  weekNum,
        success: function(data) {
              initMap(data);
        }
  });


}

</script>


